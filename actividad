class clientes:
   def __init__(self, cedula, nombre, apellido, telefono, correo, direccion, fecha_nacimiento):
        self.cedula = cedula
        self.nombre = nombre
        self.apellido = apellido
        self.telefono = telefono
        self.correo = correo
        self.direccion = direccion
        self.fecha_nacimiento = fecha_nacimiento

        def obtener_datos(self):
             return {
            "Cedula": self.cedula,
            "Nombre": self.nombre,
            "Apellido": self.apellido,
            "Telefono": self.telefono,
            "Correo": self.correo,
            "Direccion": self.direccion,
            "Fecha de Nacimiento": self.fecha_nacimiento
        }

def calcular_edad(self):
        fecha_nacimiento = datetime.strptime(self.fecha_nacimiento, "%Y/%m/%d")
        hoy = datetime.now()
        edad = hoy.year - fecha_nacimiento.year - ((hoy.month, hoy.day) < (fecha_nacimiento.month, fecha_nacimiento.day))
        return edad


cliente = Clientes(
    cedula="123456789",
    nombre="Juan diego",
    apellido="camargo perez",
    telefono="555-1234",
    correo="juan164y@gmail.com",
    direccion="Calle Falsa 123",
    fecha_nacimiento="1989/08/11"
)

# Obtener los valores de los atributos y calcular la edad
nombre_completo = f"{cliente.nombre} {cliente.apellido}"
direccion = cliente.direccion
edad = cliente.calcular_edad()

# Imprimir el mensaje deseado
print(f"Mi nombre es {nombre_completo} y vivo en {direccion} y tengo {edad} aÃ±os de edad")




